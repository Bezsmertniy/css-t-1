<header class="header">
	{{#with header}}
	<div class="d_flex">
		<div class="brandName">
			<span class="icon-brandName">{{brandName}}</span>
		</div>
		<nav class="header_nav">
			<div class="header_nav_right">
				{{#each links}}
				<a href="{{url}}" class="nav_link">{{text}}</a>
				{{/each}}
			</div>
		</nav>
	</div>
	<div class="ad">
		{{#with ad}}
		<div class="add-left">
			<a class="ad_button" href="{{url}}">Ad</a>
			<p>{{text}}</p>
		</div>
		{{/with}}
		<div class="userOnline">User online: {{userOnline}}</div>
	</div>
	{{/with}}
</header>